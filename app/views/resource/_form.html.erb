<%= form_with model: @resources, local: true do |form| %>
    <div>
        <%= form.label :name %>
        <%= form.text_field :name %>
    </div>
    <div>
        <%= form.label :description %>
        <%= form.text_field :description %>
    </div>
    <div>
        <%= form.label :url %>
        <%= form.text_field :url %>
    </div>
    <div>
        <%= form.submit %>
    </div>
<% end %>